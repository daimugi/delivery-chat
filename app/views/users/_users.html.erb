<% if users.any? %>
<ul class="list-unstyled">
  <% users.each do |user| %>
  <div class="card">
    <div class="card-header">
      <i class="fas fa-user"></i>
    </div>
    <div class="card-body">
      <%= user.name %>
    </div>
    <div>
      <p><%= link_to 'プロフィール', user_path(user) %></p>
    </div>
  </div>
  <% end %>
</ul>
<%= paginate users %>
<% end %>
